<?xml version="1.0"?>
<xs:schema id="OrqaOptions" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="OrqaOptions">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="general" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="queryPath" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="resultPath" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="templatePath" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="queryExtension" type="xs:string" minOccurs="0" maxOccurs="1" />

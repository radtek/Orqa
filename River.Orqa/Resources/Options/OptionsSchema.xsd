            <xs:sequence>
              <xs:element name="loginTimeout" type="xs:integer" minOccurs="0" maxOccurs="1" />
              <xs:element name="queryTimeout" type="xs:integer" minOccurs="0" maxOccurs="1" />
              <xs:element name="planTable" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="utilPackage" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="utilProcedure" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="promptCheck" type="xs:boolean" minOccurs="0" maxOccurs="1" />

        </xs:element>
        <xs:element name="statisticsOptions" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="classes" type="xs:integer" minOccurs="0" maxOccurs="1" />
